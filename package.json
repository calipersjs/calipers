    "chai-as-promised": "^4.3.0",
    "coveralls": "^2.11.2",
    "eslint": "^1.9.0",
    "eslint-config-lob": "^1.0.1",
    "istanbul": "^0.3.13",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.2"
  },
  "dependencies": {
    "bluebird": "^2.9.13"
